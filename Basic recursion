# Printing  1 to n integers-

void jai(int n){
   if(n==0) 
   return;
   jai(n-1);
   cout<<n<<" ";
}
    int main(){
        int n;
        cin>>n;
        jai(n);
        return 0;

    }
# Print a input name n no. of times -
void printname(string str,int n){
    if(n==0) return;
    cout<<str<<endl;
    printname(str,n-1);
}

    int main(){
       int n;
       string str;
       cin>>n;
       getline(cin,str);
       printname(str,n);
      return 0;
    }

